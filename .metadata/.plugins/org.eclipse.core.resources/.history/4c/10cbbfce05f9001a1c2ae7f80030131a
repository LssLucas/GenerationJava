package exemploInterface;

public class participanteForum implements leitor, programador{
	String pensamento;
	public String lendo(){
		return "Forum";
	}//chama
	public void pensando(char []ideias) {
		pensamento = new String (ideias);
	}//chama
	public String digitando () {
		return pensamento;
	}//cria
	public String aprendendo () {
		return "JAVA";
	}//cria
}
