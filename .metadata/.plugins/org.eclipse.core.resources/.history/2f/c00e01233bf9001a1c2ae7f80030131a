public class anotacao2 {
			static String total;
			static String tot;
			static String tota;
			

		    public static void main(String[] args)
		   {
		        // TODO Auto-generated method stub
		      String recebe=  JOptionPane.showInputDialog ("Digite um numero entre 1 e 31:\n");
		      System.out.println("Dia: " + recebe);
		        String response =JOptionPane.showInputDialog(null,"");
		        System.out.println("evento: " + response);
		        total = recebe;
		        tot = response;
		        tota = (total+" "+tot);
		      Calendar cal = new GregorianCalendar();
		      System.out.println(cal.getTime());
		  	  System.out.println(tota);
		   }
		 
}
