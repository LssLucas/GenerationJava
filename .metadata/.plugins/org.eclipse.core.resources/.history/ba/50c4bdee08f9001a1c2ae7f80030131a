package exemploInterface;

public class participanteForum implements cachorro, cavalo, preguica{
	String latindo;
	public String latindo(){
		return "AUAUAUAUA";
	}//chama
	public void pensando(char []ideias) {
		latindo = new String (ideias);
	}//chama
	public String cavalinho () {
		return latindo;
	}//cria
	public String aprendendo () {
		return "JAVA";
	}//cria
}
