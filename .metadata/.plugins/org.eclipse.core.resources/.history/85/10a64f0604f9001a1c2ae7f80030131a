package polimorfismo;

public interface cavalo extends animal implements animals{
	cavalo(String nome, int idade,String som, String mover)
	{
	super(nome, idade, som, mover, nome);
	nomeClasse="cavalo";
	}
	public String getNome() {
		return "peDePano";
	}
	public int getIdade() {
		return 21;
	}
	public String getSom() {
		return "iriririr";
	}
	public String getMover() {
		return "trotar";		
	}
}